{
    "manifest_version": 3,
    "name": "Popup de Selecci√≥n",
    "version": "1.0",
    "description": "Muestra un popup cuando seleccionas texto",
    "host_permissions": ["<all_urls>"],
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["src/Popup.css"],
        "matches" : ["<all_urls>"]
      }
    ]
  }
  