{
  "manifest_version": 3,
  "name": "Miau Miau dict",
  "version": "2.0",
  "description": "Miau miau meow mao mao üê±",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background" : {
    "service_worker":"assets/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/content.js"],
      "css":["assets/content.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["assets/*"],
    "matches": ["<all_urls>"]
  }],
  "icons":{
    "16": "icon.png",
    "48": "icon.png",
    "128":"icon.png"
  },
  "action": {
    "default_title": "Miau Miau dict"
  }
}
